package day06;

public class ArrayCopy {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]=new int[] {1,2,3,4,5}; //new를 통해서 객체를 생성
		int arr1[] = arr;
		int arr2[] = new int[5];
		//두 객체의 주소를 확인
		System.out.println(arr);
		System.out.println(arr1);
		System.out.println(arr2);
	}

}
