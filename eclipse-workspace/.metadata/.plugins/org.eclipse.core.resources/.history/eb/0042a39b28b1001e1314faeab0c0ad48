package day08;

public class Method05 {
		/* 정수 하나가 주어지면 이 정수가 소수인지 아닌지 판별(true/false);
		 * 
		 */
	public static boolean so(int num) {
		
		
		int cnt = 0 ;
		for(int i=1; i<=num; i++) {
			if(num%i==0) {
				cnt++;	
			}		
		}
			if(cnt==2) {
				return true;
			}
			else {
				return false;
			}
			 
	}

	public static void main(String[] args) {
		/* 
		 * 
		 */
		
		
		System.out.println(so(13));
	}

}
