package CardpackTest;

public class Card2 {
	private int num; 
	private char shape;
	
	public Card2() {}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
		switch(num) {
		case 1 : num = (char)'A'; break;
		case 11: num = (char)'j'; break;
		case 12: num = (char)'Q'; break;
		case 13: num = (char)'K'; break;
		default : break;
		}
	}

	public char getShape() {
		return shape;
	}

	public void setShape(char shape) {
		this.shape = shape;
	}
	
}
