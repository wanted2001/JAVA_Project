package day08;

public class Method05 {
		/* 정수 하나가 주어지면 이 정수가 소수인지 아닌지 판별(true/false);
		 * 
		 */
	public static void so(int num) {
		
		
		int cnt = 0 ;
		boolean boo =true;
		for(int i=1; i<=num; i++) {
			if(num%1==0) {
				cnt++;	
			}		
		}
		if(cnt==2) {
			System.out.println(boo);
		}
		
	}

	public static void main(String[] args) {
		/* 
		 * 
		 */
		
		so(7);
	}

}
